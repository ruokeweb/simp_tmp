<view class="content"><uni-swiper-dot vue-id="1" info="{{swiperDatas}}" current="{{current}}" mode="{{mode}}" dots-styles="{{dotsStyles}}" field="content" bind:__l="__l" vue-slots="{{['default']}}"><swiper class="swiper-box" autoplay="{{true}}" interval="{{5000}}" circular="{{true}}" data-event-opts="{{[['change',[['change',['$event']]]]]}}" bindchange="__e"><block wx:for="{{swiperDatas}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="{{['swiper-item',item.colorClass]}}"><image src="{{imagePath+item.pic}}" mode="aspectFill" data-event-opts="{{[['tap',[['goBannerDetail',['$0'],[[['swiperDatas','',index,'id']]]]]]]}}" bindtap="__e"></image></view><view class="content_wb"><view class="content_wb_jx">{{item.name}}</view></view></swiper-item></block></swiper></uni-swiper-dot><view class="kuang"></view><view class="mp-search-box"><view><m-search class="vue-ref" bind:search="__e" vue-id="2" data-ref="mSearch" data-event-opts="{{[['^search',[['search',['$event',0]]]]]}}" bind:__l="__l"></m-search></view></view><view><view class="uni-flex uni-row" style="border-bottom:1px solid #f6f6f6;"><view data-event-opts="{{[['tap',[['onClickItem',[0]]]]]}}" class="flex-item" bindtap="__e"><text class="{{[cur==0?'on':'']}}">回馈项目</text></view><view data-event-opts="{{[['tap',[['onClickItem',[1]]]]]}}" hidden="{{!(is_don_team)}}" class="flex-item" bindtap="__e"><text class="{{[cur==1?'on':'']}}">共同献爱心</text></view></view><view class="list" id="list"><block wx:for="{{donProlist}}" wx:for-item="value" wx:for-index="key" wx:key="key"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['donProlist','',key,'id']]]]]]]}}" hidden="{{!(cur===0)}}" bindtap="__e"><view class="activity clearfix"><view class="sch_tu"><block wx:if="{{value.pic!=null&&value.pic!=''}}"><image src="{{imagePath+value.pic}}" mode="aspectFill"></image></block><block wx:if="{{value.pic==null||value.pic==''}}"><image src="/static/img/don_default.png" mode="aspectFill"></image></block><block wx:if="{{value.type=='LONG_TIME'}}"><view class="doing">进行中</view></block><block wx:if="{{value.status=='HASDONE'}}"><view class="end">已结束</view></block><block wx:if="{{value.status=='BEDOING'}}"><view class="doing">进行中</view></block><block wx:if="{{value.status=='WILL'}}"><view class="readay">预备中</view></block></view><view class="sch_cont"><view class="sch_cont_title">{{''+value.name+''}}</view><block wx:if="{{null!=value.startdate&&null!=value.enddate}}"><view class="don_bm">{{''+value.summary+''}}</view></block><block wx:if="{{null==value.startdate||null==value.enddate}}"><view class="don_bm_nodate">{{''+value.summary+''}}</view></block><block wx:if="{{null!=value.startdate&&null!=value.enddate}}"><view class="don_date">{{'时间：'+value.startdate+"至"+value.enddate+''}}</view></block></view></view></view></block><block wx:for="{{teamDonList}}" wx:for-item="value" wx:for-index="key" wx:key="key"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['teamDonList','',key,'id']]]]]]]}}" hidden="{{!(cur===1&&is_don_team)}}" bindtap="__e"><view class="activity_2"><view class="sch_tu_2 clearfix"><view class="title_2">{{''+value.name+''}}</view><block wx:if="{{value.status!='SUCCESS'||value.donProject.flag!='NORMAL'}}"><view class="end_2">已结束</view></block><block wx:if="{{value.status=='SUCCESS'&&value.donProject.flag=='NORMAL'}}"><view class="doing_2">进行中</view></block></view><view class="sch_nr clearfix"><view class="bm_2"><view class="people_2">{{''+value.summary+''}}</view></view><block wx:if="{{value.smSchoolmate.truePhoto!=null&&value.smSchoolmate.truePhoto!=''}}"><image src="{{imagePath+value.smSchoolmate.truePhoto}}" mode="aspectFill"></image></block><block wx:if="{{value.smSchoolmate.truePhoto==null||value.smSchoolmate.truePhoto==''}}"><image src="/static/img/man_default.png" mode="aspectFill"></image></block></view></view></view></block></view></view><view data-event-opts="{{[['tap',[['goDon']]]]}}" hidden="{{!(cur===0&&is_don_project)}}" class="popup" bindtap="__e"><view class="popup-shade"></view><view class="suspension-hide"><image src="/static/img/don.png"></image><view style="color:#FFFFFF;">回馈</view></view></view><uni-load-more vue-id="3" status="{{status}}" contentText="{{contentText}}" color="#999999" bind:__l="__l"></uni-load-more></view>