<view class="content"><view class="content_view"><view class="content_view_item"><text class="zy_text_reg">旧手机号</text><input class="zy_input_reg" type="text" maxlength="11" placeholder="旧手机号" data-key="username" disabled="true" data-event-opts="{{[['input',[['__set_model',['','username','$event',['trim']]]]],['blur',[['$forceUpdate']]]]}}" value="{{username}}" bindinput="__e" bindblur="__e"/></view><view class="content_view_item"><text class="zy_text_reg">新手机号</text><input class="zy_input_reg" type="text" maxlength="11" placeholder="新手机号" data-key="newphone" data-event-opts="{{[['input',[['__set_model',['','newphone','$event',['trim']]]]],['blur',[['$forceUpdate']]]]}}" value="{{newphone}}" bindinput="__e" bindblur="__e"/></view><view class="content_view_item"><text class="zy_text_reg">验证码</text><input class="zy_input_reg" type="text" placeholder="请输入验证码" data-key="captChaCode" maxlength="6" data-event-opts="{{[['input',[['__set_model',['','captChaCode','$event',['trim']]]]],['blur',[['$forceUpdate']]]]}}" value="{{captChaCode}}" bindinput="__e" bindblur="__e"/><button class="yz_btn-verify" disabled="{{disabled}}" data-event-opts="{{[['tap',[['getCode',['$event']]]]]}}" bindtap="__e">{{time}}</button></view></view><view class="input-row"><button class="primary btn_dark down_but" type="primary" disabled="{{changephoneDisabled}}" data-event-opts="{{[['tap',[['save',['$event']]]]]}}" bindtap="__e">确定修改</button></view></view>