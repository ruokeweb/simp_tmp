<view><view class="uni-flex uni-row" style="border-bottom:1px solid #f6f6f6;"><view data-event-opts="{{[['tap',[['onClickItem',[0]]]]]}}" class="flex-item" bindtap="__e"><text class="{{[cur==0?'on':'']}}">未读</text></view><view data-event-opts="{{[['tap',[['onClickItem',[1]]]]]}}" class="flex-item" bindtap="__e"><text class="{{[cur==1?'on':'']}}">已读</text></view></view><view class="list" id="list"><block wx:for="{{newMessageList}}" wx:for-item="value" wx:for-index="key" wx:key="key"><view data-event-opts="{{[['tap',[['showMessage',['middle-con','$0'],[[['newMessageList','',key,'id']]]]]]]}}" hidden="{{!(cur===0)}}" class="list-cell" bindtap="__e"><view><view class="title">{{''+value.title}}</view><view class="time">{{value.createDate}}</view></view></view></block><block wx:for="{{messageList}}" wx:for-item="value" wx:for-index="key" wx:key="key"><view data-event-opts="{{[['tap',[['showMessage',['middle-con','$0'],[[['messageList','',key,'id']]]]]]]}}" hidden="{{!(cur===1)}}" class="list-cell" bindtap="__e"><view><view class="title">{{''+value.title}}</view><view class="time">{{''+value.createDate}}</view></view></view></block></view><uni-popup vue-id="1" show="{{type==='middle-con'}}" position="middle" mode="fixed" data-event-opts="{{[['^hidePopup',[['showMessage',['','']]]]]}}" bind:hidePopup="__e" bind:__l="__l" vue-slots="{{['default']}}"><view><block wx:if="{{mesContent!=''}}"><view><u-parse vue-id="{{('2')+','+('1')}}" content="{{mesContent}}" data-event-opts="{{[['^preview',[['preview']]],['^navigate',[['navigate']]]]}}" bind:preview="__e" bind:navigate="__e" bind:__l="__l"></u-parse></view></block><block wx:if="{{mesContent==''}}"><view>无内容</view></block></view></uni-popup></view>