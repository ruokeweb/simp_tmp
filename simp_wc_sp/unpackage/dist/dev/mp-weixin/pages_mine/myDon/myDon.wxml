<view class="content"><view class="top"><block wx:if="{{virtualPhoto==null||virtualPhoto==''}}"><image class="head" src="/static/img/man_default.png" mode="aspectFill"></image></block><block wx:if="{{virtualPhoto!=null&&virtualPhoto!=''}}"><image class="head" src="{{imagePath+virtualPhoto}}" mode="aspectFill"></image></block><view class="name">{{''+name+''}}</view><view class="board"><view class="item line"><view class="label">回馈金额</view><view class="num">{{null!=myInfo.myDonMoney?myInfo.myDonMoney:0}}</view></view><view class="item"><view class="label">回馈次数</view><view class="num">{{(null!=myInfo.myDonNo?myInfo.myDonNo:0)+"次"}}</view></view></view></view><view><view class="tabs"><view data-event-opts="{{[['tap',[['onClickItem',[0]]]]]}}" class="{{[cur==0?'tab on':'tab']}}" bindtap="__e">回馈</view><view data-event-opts="{{[['tap',[['onClickItem',[1]]]]]}}" class="{{[cur==1?'tab on':'tab']}}" bindtap="__e">共同献爱心</view></view><view class="list" id="list"><block wx:for="{{donProlist}}" wx:for-item="value" wx:for-index="key" wx:key="key"><block wx:if="{{cur==0}}"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['donProlist','',key]]]]]]]}}" bindtap="__e"><view class="activity"><view class="sch_tu"><block wx:if="{{value.donProject.pic!=null&&value.donProject.pic!=''}}"><image src="{{imagePath+value.donProject.pic}}" mode="aspectFill"></image></block><block wx:if="{{value.donProject.pic==null||value.donProject.pic==''}}"><image src="/static/img/don_default.png" mode="aspectFill"></image></block></view><view class="sch_cont"><view class="sch_cont_title">{{''+value.donProject.name+''}}</view><view class="date">{{''+value.time+''}}</view><view class="bm"><view class="target">回馈<text>{{value.money+"元"}}</text></view><block wx:if="{{value.state=='NORMAL'}}"><view data-event-opts="{{[['tap',[['goCertificate',['$0'],[[['donProlist','',key]]]]]]]}}" class="status certi" catchtap="__e">回馈证书</view></block><block wx:if="{{value.state=='WAITING'}}"><view data-event-opts="{{[['tap',[['goPayment',['$0'],[[['donProlist','',key]]]]]]]}}" class="status topay" catchtap="__e">支付</view></block><block wx:if="{{value.state=='HOLD'}}"><view class="status fail">回馈失败</view></block></view></view></view></view></block></block><block wx:for="{{donMyTeam}}" wx:for-item="value" wx:for-index="key" wx:key="key"><block wx:if="{{cur==1}}"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['donMyTeam','',key]]]]]]]}}" bindtap="__e"><view class="activity_2"><view class="sch_tu_2"><view class="title_2">{{''+(null==value.donProjectTogether.name?"":value.donProjectTogether.name)+''}}</view></view><view class="sch_nr clearfix"><view class="bm_2">回馈<text>{{value.money+"元"}}</text></view><block wx:if="{{trunPhoto!=null&&trunPhoto!=''}}"><image src="{{imagePath+trunPhoto}}" mode="aspectFill"></image></block><block wx:if="{{trunPhoto==null||trunPhoto==''}}"><image src="/static/img/man_default.png" mode="aspectFill"></image></block></view></view></view></block></block></view><uni-load-more vue-id="1" status="{{status}}" contentText="{{contentText}}" color="#999999" bind:__l="__l"></uni-load-more></view></view>