<view class="content"><view class="content_view"><view class="content_view_item"><text class="text_reg">学籍信息</text><block wx:if="{{flag}}"><multiple-linkage style="width:158rpx;float:right;margin-top:16rpx;" vue-id="1" showtitle="{{showTitle}}" flist="{{eduArry}}" dialogTitle="{{dialogTitle}}" showValues="{{showValueList}}" type="{{type}}" data-event-opts="{{[['^saveclick',[['savedata']]]]}}" bind:saveclick="__e" bind:__l="__l"></multiple-linkage></block></view><view class="content_view_item"><text class="text_reg">学科</text><block wx:if="{{eduflag}}"><multiple-linkage style="width:158rpx;float:right;margin-top:20rpx;" vue-id="2" showtitle="{{showeduTitle}}" flist="{{degreeTypeArry}}" dialogTitle="{{dialogeduTitle}}" showValues="{{showDegreeList}}" type="{{type}}" data-event-opts="{{[['^saveclick',[['savedata1']]]]}}" bind:saveclick="__e" bind:__l="__l"></multiple-linkage></block></view><view class="content_view_item"><text class="text_reg">班级</text><input class="input_reg" placeholder="请输入班级" placeholder-class="placeholderStyle" data-key="classes" maxlength="32" data-event-opts="{{[['input',[['__set_model',['','classes','$event',['trim']]]]],['blur',[['$forceUpdate']]]]}}" value="{{classes}}" bindinput="__e" bindblur="__e"/></view><view class="content_view_item"><text class="text_reg">学历</text><picker class="input_reg" value="{{eduRecordIndex}}" range="{{eduRecordArry}}" range-key="label" data-event-opts="{{[['change',[['bindPickerEduRecord',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{eduRecordArry[eduRecordIndex].label}}</view></picker></view><view class="content_view_item"><text class="text_reg">学位类型</text><picker class="input_reg" value="{{degreeIndex}}" range="{{degreeArry}}" range-key="label" data-event-opts="{{[['change',[['bindPickerDegree',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{degreeArry[degreeIndex].label}}</view></picker></view><view class="content_view_item"><text class="text_reg">学号</text><input class="input_reg" placeholder="请输入学号" placeholder-class="placeholderStyle" data-key="studentNo" maxlength="32" data-event-opts="{{[['input',[['__set_model',['','studentNo','$event',['trim']]]]],['blur',[['$forceUpdate']]]]}}" value="{{studentNo}}" bindinput="__e" bindblur="__e"/></view><view class="content_view_item"><text class="text_reg">学制</text><picker class="input_reg" value="{{schoollenIndex}}" range="{{schoollenArry}}" range-key="label" data-event-opts="{{[['change',[['bindPickerSchoollen',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{schoollenArry[schoollenIndex].label}}</view></picker></view><view class="content_view_item"><text class="text_reg">教育方式</text><picker class="input_reg" value="{{eduModelIndex}}" range="{{eduModelArry}}" range-key="label" data-event-opts="{{[['change',[['bindPickerEduModel',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{eduModelArry[eduModelIndex].label}}</view></picker></view><view class="content_view_item"><text class="text_reg">培养方式</text><picker class="input_reg" value="{{eduTypeIndex}}" range="{{eduTypeArry}}" range-key="label" data-event-opts="{{[['change',[['bindPickerEduType',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{eduTypeArry[eduTypeIndex].label}}</view></picker></view><view class="content_view_item"><text class="text_reg">入学日期</text><picker class="input_reg" value="{{startdate}}" mode="date" data-event-opts="{{[['change',[['bindPickerStartdate',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{null==startdate?'':startdate}}</view></picker></view><view class="content_view_item"><text class="text_reg">毕业日期</text><picker class="input_reg" value="{{enddate}}" mode="date" data-event-opts="{{[['change',[['bindPickerEnddate',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{null==enddate?'':enddate}}</view></picker></view><view class="input-row" style="margin-top:200rpx;"><button class="primary btn_dark bottom-but" type="primary" disabled="{{saveFlag}}" data-event-opts="{{[['tap',[['saveProfession',['$event']]]]]}}" bindtap="__e">保存</button><block wx:if="{{'0'!=id}}"><button class="primary btn_white bottom-but" type="default" disabled="{{deleteFlag}}" data-event-opts="{{[['tap',[['deleteProfession',['$event']]]]]}}" bindtap="__e">删除</button></block></view></view></view>