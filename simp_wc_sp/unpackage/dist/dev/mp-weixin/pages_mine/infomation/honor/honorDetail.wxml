<view class="content"><view class="content_view"><view class="content_view_item"><text class="text_reg">名称</text><input class="input_reg" placeholder="请输入名称" placeholder-class="placeholderStyle" data-key="name" maxlength="32" data-event-opts="{{[['input',[['__set_model',['','name','$event',['trim']]]]],['blur',[['$forceUpdate']]]]}}" value="{{name}}" bindinput="__e" bindblur="__e"/></view><view class="content_view_item"><text class="text_reg">行业</text><block wx:if="{{flag}}"><multiple-linkage style="width:158rpx;float:right;margin-top:20rpx;" vue-id="1" showtitle="{{showTitle}}" flist="{{industryArry}}" dialogTitle="{{dialogTitle}}" showValues="{{showIndustryList}}" type="{{type}}" data-event-opts="{{[['^saveclick',[['savedata']]]]}}" bind:saveclick="__e" bind:__l="__l"></multiple-linkage></block></view><view class="content_view_item"><text class="text_reg">获得时间</text><picker class="input_reg" value="{{date}}" mode="date" fields="year" data-event-opts="{{[['change',[['bindPickerDate',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{date==null?'':date}}</view></picker></view><view class="content_view_item"><text class="text_reg">详细说明</text><input class="input_reg" placeholder="请输入详细说明" placeholder-class="placeholderStyle" data-key="infoval" maxlength="32" data-event-opts="{{[['input',[['__set_model',['','infoval','$event',['trim']]]]],['blur',[['$forceUpdate']]]]}}" value="{{infoval}}" bindinput="__e" bindblur="__e"/></view><view class="input-row" style="margin-top:200rpx;"><button class="primary btn_dark bottom-but" type="primary" disabled="{{saveFlag}}" data-event-opts="{{[['tap',[['saveHonor',['$event']]]]]}}" bindtap="__e">保存</button><block wx:if="{{hasDelete}}"><button class="primary btn_white bottom-but" type="default" disabled="{{deleteFlag}}" data-event-opts="{{[['tap',[['deleteHonor',['$event']]]]]}}" bindtap="__e">删除</button></block></view></view></view>