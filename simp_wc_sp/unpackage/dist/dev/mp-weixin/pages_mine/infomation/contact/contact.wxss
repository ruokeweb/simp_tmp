.uni-list-cell {
  padding: 20rpx 0;
}
.uni-left {
  width: 80%;
  float: left;
}
.uni-right {
  width: 17%;
  float: right;
  padding-left: 50rpx;
  border-left: 1rpx solid #e5e5e5;
}
.uni-text {
  color: #999;
  margin-left: 20rpx;
}
.con-left {
  display: inline-block;
  width: 25%;
  position: relative;
  left: 20rpx;
}
.right-info {
  display: inline-block;
  width: 70%;
}
.button {
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 20rpx;
  border: solid 2rpx rgba(225, 182, 91, 0.4);
  text-align: center;
}
.default.button {
  background-image: -webkit-gradient(linear, left bottom, left top, from(rgba(243, 231, 200, 0.4)), to(rgba(222, 196, 144, 0.4)));
  background-image: -o-linear-gradient(bottom, rgba(243, 231, 200, 0.4) 0%, rgba(222, 196, 144, 0.4) 100%);
  background-image: linear-gradient(0deg, rgba(243, 231, 200, 0.4) 0%, rgba(222, 196, 144, 0.4) 100%);
  border: none;
}
.uni-edit {
  font-size: 30rpx;
  color: #999;
}
.con {
  padding: 0 20rpx;
}
.btn-con {
  width: 90vw;
  position: absolute;
  bottom: 70rpx;
  left: 5vw;
}

