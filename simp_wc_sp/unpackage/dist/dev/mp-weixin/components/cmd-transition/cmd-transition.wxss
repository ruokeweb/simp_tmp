.cmd-fade {
  -webkit-animation: fade .3s 1;
          animation: fade .3s 1;
}
.cmd-fade-up {
  -webkit-animation: fade-up .3s 1;
          animation: fade-up .3s 1;
}
.cmd-fade-down {
  -webkit-animation: fade-down .3s 1;
          animation: fade-down .3s 1;
}
.cmd-fade-left {
  -webkit-animation: fade-left .3s 1;
          animation: fade-left .3s 1;
}
.cmd-fade-right {
  -webkit-animation: fade-right .3s 1;
          animation: fade-right .3s 1;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
}
.cmd-slide-up {
  -webkit-animation: slide-up .3s 1;
          animation: slide-up .3s 1;
}
.cmd-slide-down {
  -webkit-animation: slide-down .3s 1;
          animation: slide-down .3s 1;
}
.cmd-slide-left {
  -webkit-animation: slide-left .3s 1;
          animation: slide-left .3s 1;
}
.cmd-slide-right {
  -webkit-animation: slide-right .3s 1;
          animation: slide-right .3s 1;
}
.cmd-bounce {
  -webkit-animation: bounce-in 0.3s linear;
          animation: bounce-in 0.3s linear;
}
.cmd-fly {
  -webkit-animation: fly-in .6s;
          animation: fly-in .6s;
  -webkit-animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
          animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
}
.cmd-punch {
  -webkit-animation: punch-in 0.3s;
          animation: punch-in 0.3s;
  -webkit-animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
          animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
}
.cmd-zoom {
  -webkit-animation: zoom-in 0.3s;
          animation: zoom-in 0.3s;
  -webkit-animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
          animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
}
@-webkit-keyframes zoom-in {
from {
    opacity: 0.01;
    -webkit-transform: scale(0.75);
